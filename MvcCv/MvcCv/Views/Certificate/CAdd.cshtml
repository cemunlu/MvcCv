@model MvcCv.Models.Entity.TblCertificates
@{
    ViewBag.Title = "CAdd";
    Layout = "~/Views/Shared/_AdminLayout.cshtml";
}


<br />
<h4>Sertifika Ekleme Sayfası</h4>
<br />
<form method="post">
    @Html.Label("Sertifika Bilgisi")
    @Html.TextBoxFor(x => x.Certificate, new { @class = "form-control" })

    @Html.Label("Tarih")
    @Html.TextBoxFor(x => x.Date, new { @class = "form-control" })
    <br />
    <button class="btn btn-success">Kaydet</button>
</form>